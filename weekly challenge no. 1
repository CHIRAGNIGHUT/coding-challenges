#include <stdio.h>
int main(){
  int arr1[100];
  int i,n,k,sum = 0;
  printf("\t\t\t\t***********************\n");
  printf("\t\t\t\tSOLUTION TO CHALLENGE 1\n");
  printf("\t\t\t\t***********************\n");
  printf("\n\n");
  printf("\thow many numbers do you wish to add?\t");
  scanf("%d",&n);
  printf("\n");
  int arr[n];
  for(i=0;i<100;i++){
    arr1[i]=0;
  }
  printf("Enter %d numbers",n);
  printf("\n");
  for(i =0;i<n;i++){
    scanf("%d",&arr[i]);
  }
  for(i=0;i<n;i++){
    k = arr[i];
    arr1[k-1]++;
  }
  for(i=0;i<100;i++){
    sum +=(arr1[i]/2);
  }
  printf("the number of pairs is %d",sum);
return 0;
}
